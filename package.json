{
  "name": "stylelint-config-react-native-styled-components",
  "version": "0.5.0",
  "main": "index.js",
  "files": [
    "index.js",
    "utils",
    "CHANGELOG.md",
    "README.md"
  ],
  "keywords": [
    "stylelint",
    "stylelint-config",
    "react-native",
    "styled-components"
  ],
  "author": "Krister Kari",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint .",
    "release": "npmpub",
    "test": "jest",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "eslint": "^8.36.0",
    "eslint-plugin-jest": "^27.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "npmpub": "^5.1.1",
    "postcss": "^8.4.21",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "stylelint": "^15.2.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-react-native": "^2.6.0"
  },
  "peerDependencies": {
    "stylelint": "^9.5.0 || ^10.0.0 || ^11.0.0 || ^12.0.0 || ^13.0.0 || ^14.0.0",
    "stylelint-react-native": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "jest"
    ],
    "env": {
      "jest/globals": true
    },
    "extends": [
      "plugin:jest/recommended"
    ]
  },
  "dependencies": {
    "compare-versions": "^5.0.3"
  }
}
