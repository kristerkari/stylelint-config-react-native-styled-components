{
  "name": "stylelint-config-react-native-styled-components",
  "version": "0.6.1",
  "main": "index.js",
  "files": [
    "index.js",
    "utils",
    "CHANGELOG.md",
    "README.md"
  ],
  "keywords": [
    "stylelint",
    "stylelint-config",
    "react-native",
    "styled-components"
  ],
  "author": "Krister Kari",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint .",
    "release": "np",
    "test": "jest",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint": "^8.49.0",
    "eslint-plugin-jest": "^27.4.3",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "np": "^8.0.4",
    "postcss": "^8.4.29",
    "postcss-styled-syntax": "^0.4.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "stylelint": "^15.10.3",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-react-native": "^2.6.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "stylelint": "^9.5.0 || ^10.0.0 || ^11.0.0 || ^12.0.0 || ^13.0.0 || ^14.0.0 || ^15.0.0",
    "stylelint-react-native": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "jest"
    ],
    "env": {
      "jest/globals": true
    },
    "extends": [
      "plugin:jest/recommended"
    ]
  },
  "dependencies": {
    "compare-versions": "^6.1.0"
  }
}
