{
  "name": "stylelint-config-react-native-styled-components",
  "version": "0.3.0",
  "main": "index.js",
  "files": [
    "index.js",
    "utils",
    "CHANGELOG.md",
    "README.md"
  ],
  "keywords": [
    "stylelint",
    "stylelint-config",
    "react-native",
    "styled-components"
  ],
  "author": "Krister Kari",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint .",
    "release": "npmpub",
    "test": "jest"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "husky": "^4.2.3",
    "jest": "^27.2.0",
    "npmpub": "^5.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "stylelint": "^13.13.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-react-native": "^2.2.0"
  },
  "peerDependencies": {
    "stylelint": "^9.5.0 || ^10.0.0 || ^11.0.0 || ^12.0.0 || ^13.0.0",
    "stylelint-react-native": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "jest"
    ],
    "env": {
      "jest/globals": true
    },
    "extends": [
      "plugin:jest/recommended"
    ]
  },
  "dependencies": {
    "compare-versions": "^3.6.0"
  }
}
